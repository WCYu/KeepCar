<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context="com.ysd.keepcar.view.personalcenter.myOrder.MyOrderActivity">

    <com.ysd.keepcar.custom.CustomTool
        android:id="@+id/toll_myorder"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@color/colorBlackText"
        android:gravity="center"
        android:paddingTop="15dp"></com.ysd.keepcar.custom.CustomTool>
    <android.support.design.widget.TabLayout
        android:id="@+id/tab_myorder"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"></android.support.design.widget.TabLayout>
    <android.support.v4.view.ViewPager
        android:id="@+id/vp_order"
        android:layout_width="match_parent"
        android:layout_height="match_parent"></android.support.v4.view.ViewPager>
</LinearLayout>
