<?xml version="1.0" encoding="utf-8"?>
<com.ysd.keepcar.custom.PercentLinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context="com.ysd.keepcar.view.personalcenter.mycar.MyCarActivity">

    <com.ysd.keepcar.custom.CustomTool
        android:id="@+id/tool_car"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@color/colorBlackText"
        android:gravity="center"
        android:paddingTop="15dp"
        app:layout_heightPercent="10%"></com.ysd.keepcar.custom.CustomTool>

    <include
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        layout="@layout/mycar_layout"/>
    <include
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        layout="@layout/mycar_layout"/>
    <include
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        layout="@layout/mycar_layout"/>

    <ListView
        android:id="@+id/listview_mycar"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"></ListView>


</com.ysd.keepcar.custom.PercentLinearLayout>
